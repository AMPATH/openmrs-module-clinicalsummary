<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="@MODULE_PACKAGE@">

	<class name="ClinicalSummaryQueueItem" table="clinical_summary_print_queue">

		<id name="clinicalSummaryQueueId" type="int" column="clinicalsummary_queue_id" 
			unsaved-value="0">
				<generator class="native" />
		</id>
		
		<many-to-one name="patient" class="org.openmrs.Patient" not-null="true" column="patient_id" />
		
		<many-to-one name="location" class="org.openmrs.Location" not-null="true" column="location_id" />
		
		<property name="encounterDatetime" column="encounter_datetime" 
			type="java.util.Date" not-null="true" length="19" />
	
		<property name="status" type="java.lang.String" length="64" access="field" />

		<property name="fileName" column="file_name" 
			type="java.lang.String" length="255" />
		
		<property name="errorMessage" column="error_message" 
			type="java.lang.String" length="512" />
		
		<property name="datePrinted" column="date_printed" 
			type="java.util.Date" not-null="false" length="19" />
		
		<property name="dateCreated" column="date_created" 
			type="java.util.Date" not-null="false" length="19" />
		
	</class>
</hibernate-mapping>
