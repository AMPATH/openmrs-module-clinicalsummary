<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="@MODULE_PACKAGE@">

	<class name="WeightAgeStandard" table="clinical_summary_weight_age">

		<id name="standardId" type="java.lang.Integer" column="standard_id" unsaved-value="0">
			<generator class="org.openmrs.api.db.hibernate.NativeIfNotAssignedIdentityGenerator" />
		</id>
        <property name="lValue" type="java.lang.Double" column="l_value"/>
        <property name="mValue" type="java.lang.Double" column="m_value"/>
        <property name="sValue" type="java.lang.Double" column="s_value"/>
        <property name="gender" type="java.lang.String" column="gender"/>
        <property name="ageUnit" type="java.lang.String" column="age_unit"/>
        <property name="age" type="java.lang.Integer" column="age"/>
		
	</class>
</hibernate-mapping>
